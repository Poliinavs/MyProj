@using WebApplication1.Data.Models
@model IEnumerable<Car>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <h2>Все автомобили</h2>
    <h3>ViewBag.Category</h3>
    @{
        foreach (var car in Model)
        {
            <p>Модель: @car.Name</p>
            <p>Цена: @car.price.ToString("c")</p>
        }
    }
</body>
</html>
